<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tableplay.poker">

    <application
        android:name="jp.playing.table.poker.TaskApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="PokerData"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WinnerCheckActivity"></activity>
        <activity
            android:name=".AddMemberListActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".DataDetailActivity"/>
        <activity android:name=".ShowDownActivity"/>
        <activity android:name=".CardActivity"/>
        <activity android:name=".PlayingActivity"/>
        <activity android:name=".MemberPlayingActivity"/>
        <activity android:name=".HandActivity"/>
        <activity android:name=".AddMemberActivity"/>
        <activity
            android:name=".TurnActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".SettingActivity"/>
        <activity android:name=".TitleActivity"/>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>